{
    "name": "fholznagel/notifier",
    "description": "Notifier plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "3.6.*"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "2.*",
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Fholznagel\\Notifier\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fholznagel\\Notifier\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
